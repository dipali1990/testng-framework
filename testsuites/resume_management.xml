    <test name="Verify Job Seeker is able to create resume using Upload Resume File option">
        <parameter name="siteID" value="23014"/>
        <classes>
            <class name="com.ymcareers.webapp.tests.jobseeker.manageresume.ManageResumeTest">
                <methods>
                    <include name="createResumeUsingUploadFileOption"/>
